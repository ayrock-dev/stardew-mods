        "enableGrass": {
            "AllowValues": "true, false",
            "Default": "true"
        },
        "enableDigSpot": {
            "AllowValues": "true, false",
            "Default": "true"
        },
        "enableFruitTrees":{
            "AllowValues":"true,false",
            "Default":"true"
        },
        "enableMiniMap":{
            "AllowValues":"true, false",
            "Default":"true"
        },
        "enableTownBuildings":{
            "AllowValues":"true, false",
            "Default":"true"
        },
        "treesVersion":{
            "AllowValues":"vanilla,new",
            "Default":"new"
        },
        "winterTilledSoil":{
            "AllowValues":"brown,blue",
            "Default":"brown"
        }
